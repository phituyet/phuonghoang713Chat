<h1> Messages</h1>
<table class="table table-stripped">
  <thred class="thread thread-default">
    <tr>
      <th>
        Sender
      </th>
      <th>
        Subject
      </th>
      <th>
        Body
      </th>
    </tr>
  </thred>
  <body>
    <% @messages.each do |message| %>
    <tr>
      <td><%= message.sender.name %></td>
      <td><%= message.subject %></td>
      <td><%= message.body %></td>
    </tr>
    <% end %>
  </body>
</table>
